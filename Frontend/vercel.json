{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/login", "destination": "/pages/login/login1.html" },
    { "source": "/register", "destination": "/pages/register/register.html" },
    { "source": "/password", "destination": "/pages/password/password.html" },
    { "source": "/perfil-configuracion", "destination": "/pages/perfil-configuracion/perfil-configuracion.html" },
    { "source": "/terminoscondiciones", "destination": "/pages/terminoscondiciones/terminos.html" },
    { "source": "/cursos", "destination": "/pages/cursos/cursos.html" },
    { "source": "/cursosdash", "destination": "/pages/cursos/cursosdash.html" },
    { "source": "/dashboard", "destination": "/pages/dashboard/dashboard.html" },
    { "source": "/pagos", "destination": "/pages/pagos/payment.html" },
    { "source": "/leccion-adultos", "destination": "/pages/lecciones/leccion-adultos.html" },
    { "source": "/leccion-naturaleza", "destination": "/pages/lecciones/leccion-naturaleza.html" },
    { "source": "/leccion-ninos", "destination": "/pages/lecciones/leccion-ninos.html" },
    { "source": "/leccion-yoga", "destination": "/pages/lecciones/leccion-yoga.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

